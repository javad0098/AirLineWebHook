# Ignore Visual Studio temporary files, build results, and
# user-specific files.
.vscode/
.vs/
*.user
*.suo
*.userosscache
*.sln.docstates

# Ignore build results
bin/
obj/
*.exe
*.dll
*.pdb
*.app

# Ignore NuGet packages
*.nupkg
*.snupkg
packages/
.nuget/
project.lock.json
project.fragment.lock.json
artifacts/

# Ignore the folder where your Docker images are stored
dockercompose/docker-compose.override.yml
dockercompose/docker-compose.override.*.yml

# Ignore Rider (JetBrains) IDE files
.idea/

# Ignore log files
*.log

# Ignore npm dependencies (if you happen to use front-end dependencies)
node_modules/

# Ignore OS generated files
.DS_Store
Thumbs.db

# Ignore database files (example if you have SQLite or other local DB)
*.mdf
*.ldf
*.sqlite
*.sqlite3

# Ignore Python virtual environments (if applicable)
__pycache__/
*.pyc
env/

# Ignore by default git-specific files that are not needed
*.git/
!.gitignore
